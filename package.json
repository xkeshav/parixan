{
  "name": "parixan",
  "version": "1.0.0",
  "description": "parcel",
  "default": "src/html/index.html",
  "scripts": {
    "serve": "NODE_ENV=dev nodemon dist/index.js",
    "prebuild": "rimraf dist .parcel-cache",
    "build": "parcel build src/html/*.html --public-url ./",
    "start": "parcel src/html/*.html --lazy",
    "watch": "parcel watch --no-hmr",
    "dev": "npm run start && node ./server/index.js --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xkeshav/parixan.git"
  },
  "keywords": [
    "parcel",
    "parixan"
  ],
  "author": "Keshav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xkeshav/parixan/issues"
  },
  "homepage": "https://github.com/xkeshav/parixan#readme",
  "browserslist": "> 0.5%, last 2 versions",
  "devDependencies": {
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "parcel": "^2.9.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "url": "^0.11.1",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@babel/register": "^7.22.5",
    "@babel/runtime-corejs3": "^7.22.6",
    "@parcel/babel-plugin-transform-runtime": "^2.9.3",
    "@parcel/babel-preset-env": "^2.9.3",
    "core-js": "^2.5.7",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "rimraf": "^5.0.1"
  },
  "@parcel/bundler-default": {
    "minBundles": 1,
    "minBundleSize": 3000,
    "maxParallelRequests": 20
  }
}
